<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">
	
	<Description about="urn:mozilla:install-manifest">
		<em:id>frangoudes.fotos@gmail.com</em:id>
		<em:name>Duplicates Merger for Zotero</em:name>
		<em:version>1.0.0</em:version>
		<em:description>A plugin to merge duplicate items in Zotero</em:description>
		<em:creator>Fotos Frangoudes</em:creator>
		<em:updateURL>https://github.com/frangoud/ZoteroDuplicatesMerger</em:updateURL> <!-- https://www.zotero.org/download/plugins.rdf -->
		<em:type>2</em:type> <!-- type=extension -->
		
		<!-- Requires Zotero -->
		<em:requires>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:name>Zotero</em:name>
				<em:homepageURL>http://www.zotero.org</em:homepageURL>
				<em:minVersion>5.0</em:minVersion>
				<em:maxVersion>*</em:maxVersion>
			</Description>
		</em:requires>
    
		<!-- Requires Firefox or Zotero Standalone -->		
		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>5.0</em:minVersion>
				<em:maxVersion>*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>
</RDF>
